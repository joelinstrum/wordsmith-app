{
  "expo": {
    "name": "wordsmith-app",
    "slug": "wordsmith-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "permissions": [
      "CAMERA_ROLL",
      "READ_EXTERNAL_STORAGE",
      "WRITE_EXTERNAL_STORAGE"
    ],
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#222"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "permissions": [
        "NSFileProtectionNone"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#222"
      },
      "permissions": [
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.joe44850.wordsmithapp"
    },
    "web": {
      "favicon": "./assets/images/icon.png"
    },
    "resolver": {
      "assetExts": [
        "db",
        "ttf",
        "png",
        "json",
        "jpeg",
        "jpg",
        "svg"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "cc889dbd-8646-40e5-a060-0b7fb6e865ba"
      }
    }
  }
}
